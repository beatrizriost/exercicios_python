# Entrada
num1 = int(input(f'\n\tInsira os valores:\n\tNúmero 1: '))
num2 = int(input(f'\n\tNúmero 2: '))

# Processamento
ad = num1 + num2 
sub = num1 - num2
mut = num1 * num2
div = num1/num2

# Saída
print(f'\n\t {num1} + {num2} = {ad}')
print(f'\n\t {num1} - {num2} = {sub}')
print(f'\n\t {num1} x {num2} = {mut}')
print(f'\n\t {num1} / {num2} = {div}')
